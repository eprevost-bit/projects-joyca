<odoo>

    <record id="stock_picking_form_proyecto_linea" model="ir.ui.view">
        <field name="name">stock.picking.form.proyecto.linea</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">


            <xpath expr="//field[@name='move_ids_without_package']"
                   position="attributes">
                <attribute name="class">text-center</attribute>
            </xpath>


            <xpath expr="//field[@name='move_ids_without_package']//field[@name='quantity']"
                   position="before">
                <field name="proyecto_id" class="text-center"/>
            </xpath>

        </field>
    </record>

</odoo>

